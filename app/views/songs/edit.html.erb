<h1>Edit Song Form</h1>
<% if @song.errors.any? %>
    <% @song.errors.full_messages.each do |msg| %>
        <%= msg %> 
    <% end %>
<% end %>
<br>
<%= form_for @song do |f| %>

    <%= f.label "Song Title" %>
    <%= f.text_field :title %><br>

    <%= f.label "Released" %>
    <%= f.check_box :released %><br>

    <%= f.label "Release Year" %>
    <%= f.text_field :release_year %><br>

    <%= f.label "Artist Name" %>
    <%= f.text_field :artist_name %><br>

    <%= f.label "Genre" %>
    <%= f.text_field :genre %><br>

    <%= f.submit %>

<% end %>