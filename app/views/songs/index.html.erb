<h1>Listing of Songs</h1>

<table>
    <tr>
        <th>Title</th>
        <th>Released</th>
        <th>Release Year</th>
        <th>Artist</th>
        <th>Genre</th>
    </tr>
    <% @songs.each do |song| %>
        <tbody>
        <tr>
            <td><%= song.title %></td>
            <td><%= song.released %></td>
            <td><%= song.release_year %></td>
            <td><%= song.artist_name %></td>
            <td><%= song.genre %></td>
        </tr>
        </tbody>
    <% end %>
</table>